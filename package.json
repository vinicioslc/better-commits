    "url": "git+https://github.com/vinicioslc/better-commits.git"
  },
  "keywords": [
    "commitlint",
    "ci",
    "test"
  ],
  "author": "vinicioslc@outlook.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vinicioslc/better-commits/issues"
  },
  "homepage": "https://github.com/vinicioslc/better-commits#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "commitizen": "^4.2.4",
    "commitlint-config-gitmoji": "^2.2.5",
    "cz-customizable": "^6.3.0",
    "husky": "^7.0.0"
  }
}
