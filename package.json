{
  "name": "better-commits",
  "version": "1.0.0",
  "description": "This npm package manages the commit linting and other developer stuffs for the repo",
  "main": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "cm": "cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "repository": {
